    "graphql": "^15.5.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "react-bootstrap": "^1.5.2",
    "react-hook-form": "^7.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
